11 to s11 + 10elsedim b11 as number = s12 - s11 - 20dim l11 as number = s11 + 10endifendifif(anzahlspalten > 11)if(HasVariable("b12"))set b12 to s13 - s12 - 20set l12 to s12 + 10elsedim b12 as number = s13 - s12 - 20dim l12 as number = s12 + 10endifendifif(anzahlspalten > 12)if(HasVariable("b13"))set b13 to s14 - s13 - 20set l13 to s13 + 10elsedim b13 as number = s14 - s13 - 20dim l13 as number = s13 + 10endifendifif(anzahlspalten > 13)if(HasVariable("b14"))set b14 to s15 - s14 - 20set l14 to s14 + 10elsedim b14 as number = s15 - s14 - 20dim l14 as number = s14 + 10endifendifif(anzahlspalten > 14)if(HasVariable("b15"))set b15 to s15 - s15 - 20set l15 to s15 + 10elsedim b15 as number = s16 - s15 - 20dim l15 as number = s15 + 10endifendifif(anzahlspalten > 15)if(HasVariable("b16"))set b16 to s17 - s16 - 20set l16 to s16 + 10elsedim b16 as number = s17 - s16 - 20dim l16 as number = s16 + 10endifendifif(anzahlspalten > 16)if(HasVariable("b17"))set b17 to s18 - s17 - 20set l17 to s17 + 10elsedim b17 as number = s18 - s17 - 20dim l17 as number = s17 + 10endifendifif(anzahlspalten > 17)if(HasVariable("b18"))set b18 to s19 - s18 - 20set l18 to s18 + 10elsedim b18 as number = s19 - s18 - 20dim l18 as number = s18 + 10endifendifif(anzahlspalten > 18)if(HasVariable("b19"))set b19 to s20 - s19 - 20set l19 to s19 + 10elsedim b19 as number = s20 - s19 - 20dim l19 as number = s19 + 10endifendif0000011